@article{roberts2000,
  title = {{{NGO Leadership}}, {{Success}}, and {{Growth}} in {{Senegal}}: {{Lessons From Ground Level}}},
  volume = {29},
  issn = {0894-6019},
  url = {http://www.jstor.org.proxy.ub.uni-frankfurt.de/stable/40553380},
  shorttitle = {{{NGO Leadership}}, {{Success}}, and {{Growth}} in {{Senegal}}},
  abstract = {This ethnographic account of the NGO Rodale International Senegal illustrates linkages among leadership, success and growth in the promotion and adoption of regenerative agricultural practices in the Republic of Senegal between the years 1987 and 1999. Observations carried out over a seven year period (1993-present), particularly those in times of "crisis" or "opportunity," have revealed much about the micropolitics of this organization. Under pressure from structural adjustment reforms and unable to reverse trends leading to increased environmental degradation, the Senegalese government and farmers were receptive to the innovations brought by the Rodale Institute through their Regenerative Agriculture Resource Center (RARC) in Thiès. Three areas of NGO intervention are documented: education, communication and information exchange; collaborative applied research; and participatory community development. The ability of staff members to foster participation at multiple levels among members of Senegalese society and the international development community contributed greatly to their recognition as a leader among NGOs. The paper concludes by listing the factors that contributed to this NGO's success in Senegal, and attempts to draw lessons for anthropologists who work with NGOs about the inevitability of becoming involved in a highly politicized arena of collaboration and competition among these organizations and their staff.},
  timestamp = {2017-06-26T08:38:05Z},
  number = {2},
  journaltitle = {Urban Anthropology and Studies of Cultural Systems and World Economic Development},
  author = {Roberts, Bill},
  date = {2000},
