@article{berthelemy1980,
  title = {L'économie de l'Afrique occidentale française et du Togo, 1946-1960},
  volume = {67},
  url = {http://www.persee.fr/doc/outre_0300-9513_1980_num_67_248_2263},
  doi = {10.3406/outre.1980.2263},
  abstract = {La période qui s'étend de la fin de la Seconde Guerre mondiale aux indépendances apparaît, globalement, comme une période de relative prospérité pour l'ensemble économique constitué par l'A.O.F. et le Togo. Cependant, la progression d'ensemble des productions et du niveau de vie a été assez inégale, à de nombreux égards. Cet article étudie, à partir d'un rassemblement des sources statistiques disponibles, les facteurs qui ont pu influencer l'activité économique et l'ampleur de leurs effets sur l'évolution économique d'ensemble. Deux facteurs paraissent pouvoir être retenus : la politique économique décidée par la Métropole et la croissance des exportations. Le rôle décisif joué par la politique économique est tout d'abord mis en évidence. Son instrument principal a été le Plan : dans le cadre du Plan, des transferts d'aide ont été accordés aux Territoires d'Outre-Mer pour des montants assez importants, des investissements publics ont été réalisés, qui ont influencé assez profondément l'économie. La croissance des activités d'exportation a été bien sûr marquée par la politique économique, mais aussi par les tendances des cours mondiaux et par les aléas climatiques. La production de denrées exportables et les revenus qui lui sont liés ont ainsi suivi des évolutions très marquées, réagissant, de façon plus ou moins sensible selon les cas, sur l'ensemble de l'économie.},
  timestamp = {2017-08-02T02:08:56Z},
  langid = {french},
  number = {248},
  journaltitle = {Revue française d'histoire d'outre-mer},
  author = {Berthélemy, Jean-Claude},
  urldate = {2017-08-02},
  date = {1980},
  pages = {301--337},
  file = {berthélemy-1980-l'économie_de_l'afrique_occidentale_française_et_du_togo,_1946-1960.pdf:C\:\\Users\\Philipp Schweizer\\OneDrive\\zotero-files\\berthélemy-1980-l'économie_de_l'afrique_occidentale_française_et_du_togo,_1946-1960.pdf:application/pdf},
  groups = {Magisterarbeit}
}

